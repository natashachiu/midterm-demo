<%- include('partials/_header') %>
  <div id="story_div" class="container-fluid">
    <div id="stories" class="row row-cols-1 row-cols-md-3 row-cols-lg-3 g-lg-5 g-sm-0">
<% for (let story of stories) {%>

  <div class="col">
    <div class="card" style="box-sizing: 10em; border-color: rgba(59, 59, 59, 0.161);">
      <a href="/story/<%=story.id%>/contribute" style="color: black; text-decoration: none;">
        <div class="card-body">
          <h2 class="card-title">
            <%=story.title%>
          </h2>
          <span class="card-subtitle"><span style="font-size: medium; opacity: 60%;">By <%=story.username%>
            </span><span>
              <%=story.complete%>
            </span></span>
          <div class="card-subtitle">
            <%= story.description %>
          </div>
          <div class="text"></div>
        </div>
    </div>
    </a>
  </div>
  <%}%>
</div>
</div>


</body>

</html>